<h1>Sign up</h1>
<form>
  <div class="form-group row">
    <label for="name" class="col-md-2">Name</label>
    <div class="col-md-10">
      <input type="text" class="form-control form-control-plaintext" id="name" value="Mariona" readonly>
    </div>
  </div>
  <div class="form-group row">
    <label for="lastName" class="col-md-2">Last name</label>
    <div class="col-md-10">
      <input type="text" class="form-control form-control-plaintext" id="lastName" value="Roca Ros" readonly>
    </div>
  </div>
  <div class="form-group row">
    <label for="userName" class="col-md-2">Username</label>
    <div class="col-md-10">
      <input type="text" class="form-control form-control-plaintext" id="userName" value="marionaroca84" readonly>
    </div>
  </div>
  <div class="form-group row">
    <label for="password" class="col-md-2">Password</label>
    <div class="col-md-10">
      <input type="password" class="form-control" id="password" value="1234">
    </div>
  </div>
  <div class="form-group row">
    <label for="mail" class="col-md-2">Mail</label>
    <div class="col-md-10">
      <input type="mail" class="form-control" id="mail">
    </div>
  </div>
  <div class="form-group address">
    <div class="label-address">Address</div>
    <div class="address-group row">
      <label for="roadType" class="col-md-3">Road type</label>
      <div class="col-md-9">
        <input type="text" class="form-control" id="roadType">
      </div>
    </div>
    <div class="address-group row">
      <label for="roadName" class="col-md-3">Road name</label>
      <div class="col-md-9">
        <input type="text" class="form-control" id="roadName">
      </div>
    </div>
    <div class="address-group row">
      <label for="number" class="col-md-3">Number</label>
      <div class="col-md-9">
        <input type="text" class="form-control" id="number">
      </div>
    </div>
    <div class="address-group row">
      <label for="zipCode" class="col-md-3">Zip code</label>
      <div class="col-md-9">
        <input type="text" class="form-control" id="zipCode">
      </div>
    </div>
    <div class="address-group row">
      <label for="city" class="col-md-3">City</label>
      <div class="col-md-9">
        <input type="text" class="form-control" id="city">
      </div>
    </div>
    <div class="address-group row">
      <label for="province" class="col-md-3">Province</label>
      <div class="col-md-9">
        <input type="text" class="form-control" id="province">
      </div>
    </div>
    <div class="address-group row">
      <label for="state" class="col-md-3">State</label>
      <div class="col-md-9">
        <input type="text" class="form-control" id="state">
      </div>
    </div>
  </div>
  <div class="form-group row">
    <label for="contactTel" class="col-md-2">Contact Telephone</label>
    <div class="col-md-10">
      <input type="mail" class="form-control" id="contactTel">
    </div>
  </div>
  <div class="form-group row">
    <label for="personalIntroducing" class="col-md-2">Introduce Yourself</label>
    <div class="col-md-10">
      <textarea class="form-control" id="personalIntroducing"></textarea>
    </div>
  </div>
  <div class="form-group row">
    <label for="image" class="col-md-2">Profile Image</label>
    <div class="col-md-10">
      <input type="file" class="form-control-file" id="image">
    </div>
  </div>
  <button type="submit" class="btn-lg btn-primary">Sign up</button>
</form>